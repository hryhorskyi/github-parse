<div id="<%= dom_id user %>">
  <p>
    <strong>Login:</strong>
    <%= user.login %>
  </p>

  <p>
    <strong>Full_name:</strong>
    <%= user.full_name %>
  </p>

  <p>
    <strong>Repos:</strong>
    <br>

    <% user.repos.each do |t| %>
      - 
      <%= t %><br/>
    <% end %>
  </p>

</div>
